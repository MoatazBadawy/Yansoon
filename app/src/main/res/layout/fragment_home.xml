<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toTopOf="parent">

        <ImageView
            android:id="@+id/image_photo1"
            android:layout_width="160dp"
            android:layout_height="106dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="8dp"
            android:scaleType="fitXY"
            android:src="@drawable/cairo_tower"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/image_photo2"
            android:layout_width="160dp"
            android:layout_height="106dp"
            android:layout_marginTop="8dp"
            android:scaleType="fitXY"
            android:src="@drawable/egyptian_museum"
            app:layout_constraintEnd_toEndOf="@+id/image_photo1"
            app:layout_constraintStart_toStartOf="@+id/image_photo1"
            app:layout_constraintTop_toBottomOf="@+id/image_photo1" />

        <ImageView
            android:id="@+id/image_photo3"
            android:layout_width="152dp"
            android:layout_height="220dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="16dp"
            android:scaleType="fitXY"
            android:src="@drawable/coptic_museum"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/sekoseko"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout">

        <ImageView
            android:id="@+id/image_photo4"
            android:layout_width="152dp"
            android:layout_height="106dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="16dp"
            android:scaleType="fitXY"
            android:src="@drawable/koshary_abu__tareq"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/image_photo5"
            android:layout_width="152dp"
            android:layout_height="106dp"
            android:layout_marginTop="8dp"
            android:scaleType="fitXY"
            android:src="@drawable/the_grill"
            app:layout_constraintEnd_toEndOf="@+id/image_photo4"
            app:layout_constraintStart_toStartOf="@+id/image_photo4"
            app:layout_constraintTop_toBottomOf="@+id/image_photo4" />

        <ImageView
            android:id="@+id/image_photo6"
            android:layout_width="160dp"
            android:layout_height="220dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="8dp"
            android:scaleType="fitXY"
            android:src="@drawable/taboula"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        app:layout_constraintTop_toBottomOf="@+id/sekoseko">

        <ImageView
            android:id="@+id/image_photo1"
            android:layout_width="160dp"
            android:layout_height="106dp"
            android:layout_marginStart="16dp"
            android:layout_marginTop="8dp"
            android:scaleType="fitXY"
            android:src="@drawable/cairo_tower"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <ImageView
            android:id="@+id/image_photo2"
            android:layout_width="160dp"
            android:layout_height="106dp"
            android:layout_marginTop="8dp"
            android:scaleType="fitXY"
            android:src="@drawable/egyptian_museum"
            app:layout_constraintEnd_toEndOf="@+id/image_photo1"
            app:layout_constraintStart_toStartOf="@+id/image_photo1"
            app:layout_constraintTop_toBottomOf="@+id/image_photo1" />

        <ImageView
            android:id="@+id/image_photo3"
            android:layout_width="152dp"
            android:layout_height="220dp"
            android:layout_marginTop="8dp"
            android:layout_marginEnd="16dp"
            android:scaleType="fitXY"
            android:src="@drawable/coptic_museum"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>

